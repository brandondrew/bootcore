<h1>Listing cloud_configs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Accessibility</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cloud_configs.each do |cloud_config| %>
      <tr>
        <td><%= cloud_config.user %></td>
        <td><%= cloud_config.accessibility %></td>
        <td><%= link_to 'Show', cloud_config %></td>
        <td><%= link_to 'Edit', edit_cloud_config_path(cloud_config) %></td>
        <td><%= link_to 'Destroy', cloud_config, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cloud config', new_cloud_config_path %>
